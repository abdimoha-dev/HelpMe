        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@sedvels/platzom": "^0.1.0",
        "browser-sync": "^2.24.4",
        "jquery-slim-webpack": "0.0.3",
        "jquery-thin": "^3.2.3",
        "unofficial-light-bootstrap-dashboard": "^1.0.0"
    }
